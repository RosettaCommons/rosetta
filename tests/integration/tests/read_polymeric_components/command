# change to working directory
cd %(workdir)s

# check to see if executable exits
 [ -x %(bin)s/dump_capped_residue.%(binext)s ] || exit 1

# run executable
%(bin)s/dump_capped_residue.%(binext)s -residue_name pdb_02L -PDB_components_file components_trimmed.cif -load_PDB_components -database %(database)s -testing:INTEGRATION_TEST  2>&1 \
     | egrep -vf ../../ignore_list \
     > log1
%(bin)s/dump_capped_residue.%(binext)s -residue_name pdb_125 -PDB_components_file components_trimmed.cif -load_PDB_components -extra_patch_fa %(database)s/chemical/residue_type_sets/fa_standard/patches/nucleic/rna/3prime5prime_methyl_phosphate.txt -database %(database)s -testing:INTEGRATION_TEST  2>&1 \
     | egrep -vf ../../ignore_list \
     > log2
%(bin)s/dump_capped_residue.%(binext)s -residue_name pdb_0C -PDB_components_file components_trimmed.cif -load_PDB_components -extra_patch_fa %(database)s/chemical/residue_type_sets/fa_standard/patches/nucleic/rna/3prime5prime_methyl_phosphate.txt -database %(database)s -testing:INTEGRATION_TEST  2>&1 \
     | egrep -vf ../../ignore_list \
     > log3

# test to see if exe terminated abnormaly
test "${PIPESTATUS[0]}" != '0' && exit 1 || true
