<ROSETTASCRIPTS>
    <SCOREFXNS>
    </SCOREFXNS>
    <RESIDUE_SELECTORS>
	<Index name="fixedres" resnums="1-9,16-20"/>
	<Index name="loop" resnums="10-15"/>
    </RESIDUE_SELECTORS>
    <PACKER_PALETTES>
    </PACKER_PALETTES>
    <TASKOPERATIONS>
	<OperateOnResidueSubset name="norepack" selector="fixedres" >
	    <PreventRepackingRLT/>
	</OperateOnResidueSubset>
    </TASKOPERATIONS>
    <MOVE_MAP_FACTORIES>
    </MOVE_MAP_FACTORIES>
    <SIMPLE_METRICS>
    </SIMPLE_METRICS>
    <FILTERS>
    </FILTERS>
    <MOVERS>
	<MultipleOutputWrapper name="try_rotamer" max_output_poses="10" keep_mover_state="true" >
		<TryRotamers res_num="6" automatic_connection="false" jump_num="0" explosion="0" include_current="false"/>
	</MultipleOutputWrapper>
	<PertMinMover name="pertmin" residue_selector="loop" />
	<MultiplePoseMover name="multiple_pose" mover="pertmin" />
	<PackRotamersMover name="design" task_operations="norepack" />
    </MOVERS>

    <PROTOCOLS>
	<Add mover="try_rotamer" />
        <FOR_EACH_POSE>
            <PROTOCOLS>
		<Add mover="multiple_pose"/>
                <FOR_EACH_POSE>
		   <PROTOCOLS>
                       <Add mover="design"/>
                   </PROTOCOLS>
                </FOR_EACH_POSE>
            </PROTOCOLS>
        </FOR_EACH_POSE>    	
    </PROTOCOLS>

    <OUTPUT />
</ROSETTASCRIPTS>
