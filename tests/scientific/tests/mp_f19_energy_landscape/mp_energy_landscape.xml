	<TASKOPERATIONS>
			<RestrictToRepacking name="rtrp"/>
			<ExtraRotamersGeneric name="extra_chi" ex1="1" ex2="1" extrachi_cutoff="0"/>
	</TASKOPERATIONS>
	<MOVERS>
		<AddMembraneMover name="add_memb" />
		<TransformIntoMembraneMover name="transform_into_memb" />
