%(bin)s/version_mpscorefxn.%(binext)s @flags -database %(database)s -run:constant_seed -nodelay -sfxnfprnt::output_fingerprint_file membrane_fa_scorefxn.fprnt >& log

test "${PIPESTATUS[0]}" != '0' && exit 1 || true  # Check if the first executable in pipe line return error and exit with error code if so

#only compare the fingerprint file, don't compare the log file
rm log

